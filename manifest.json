{
  "manifest_version": 2,
  "name": "radhta-fork close duplicate tabs",
  "version": "0.3",
  "homepage_url": "https://github.com/radhta/tab-closer",
  "applications": {
    "gecko": {
      "id": "tab-closer@radhta"
    }
  },
  "browser_action": {
      "default_title": "Close duplicate tabs",
      "default_icon": {
        "48": "icon.svg"
    }
  },
  "background": {
      "scripts": ["background.js"]
  },
  "icons": {
      "48": "icon.svg"
  },
  "permissions": [
      "cookies",
      "notifications",
      "tabs"
  ]
}

